{
	"name": "_toJSON",
	"properties": {
		"type": "Flowlet",
		"typeProperties": {
			"sources": [],
			"sinks": [],
			"transformations": [
				{
					"name": "aggregate"
				},
				{
					"name": "select"
				},
				{
					"name": "deriveData"
				},
				{
					"name": "input"
				},
				{
					"name": "output"
				}
			],
			"scriptLines": [
				"parameters{",
				"     fields as string",
				"}",
				"input(output(",
				"          data as ()",
				"     ),",
				"     order: 0,",
				"     allowSchemaDrift: true) ~> input",
				"deriveData aggregate(groupBy(dummy = 1),",
				"     data = collect(data)) ~> aggregate",
				"aggregate select(mapColumn(",
				"          data",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> select",
				"input derive(data = $fields) ~> deriveData",
				"select output() ~> output"
			]
		}
	}
}